{
  "name": "cs326-final-hay",
  "version": "1.0.0",
  "description": "First version of haystation",
  "main": "server.js",
  "engines": {
    "node": "12.x"
  },
  "directories": {
    "doc": "docs"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.11.0",
    "http-server": "^0.12.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "lint": "eslint client/ server/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Martin-Dickie/cs326-final-hay.git"
  },
  "author": "Martin Dickie, Anthony Rinaldi, Shivangi Khanna",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Martin-Dickie/cs326-final-hay/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "consistent-return": "error",
      "no-var": "error",
      "eqeqeq": "error",
      "semi": [
        "error",
        "always"
      ],
      "curly": [
        "warn",
        "all"
      ],
      "prefer-const": [
        "warn",
        {}
      ]
    }
  },
  "homepage": "https://github.com/Martin-Dickie/cs326-final-hay#readme"
}
